{
  "name": "backend-highload-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon src/index.js",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate:create": "./node_modules/.bin/sequelize migration:create --name create-table-users",
    "migrate:down": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "db_prepare": "npm run db:create & npm run migrate & npm run db:seed:all",
    "db:seed:all": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "nodemon": "^3.0.1"
  },
  "type": "module"
}
